generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views", "strictUndefinedChecks", "relationJoins", "nativeDistinct", "omitApi", "prismaSchemaFolder", "typedSql"]
  binaryTargets   = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
