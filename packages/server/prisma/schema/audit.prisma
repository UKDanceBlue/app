model AuditLog {
  id              Int      @id @default(autoincrement())
  uuid            String   @unique(map: "AuditLog_uuid_unique") @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  summary         String
  details         Json
  userId          Int?     @map("editingUserId")
  subjectGlobalId String?
  createdAt       DateTime @default(now()) @db.Timestamptz(6)
  user            Person?  @relation(fields: [userId], references: [id], map: "AuditLog_editingUserId_Person_id_fk")
}
