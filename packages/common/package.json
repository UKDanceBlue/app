    "luxon": "^3.5.0",
    "reflect-metadata": "^0.2.2",
    "ts-results-es": "^4.2.0",
    "type-graphql": "^2.0.0-rc.2",
    "typedi": "^0.10.0",
    "utility-types": "^3.11.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/react": "~18.3.12",
    "@types/validator": "^13.12.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "packageManager": "yarn@4.5.1"
