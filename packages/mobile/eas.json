{
  "cli": {
    "version": ">=5.9.1",
    "requireCommit": true
  },
  "build": {
    "dev-client": {
      "distribution": "internal",
      "releaseChannel": "development",
      "developmentClient": true,
      "cache": {
        "key": "4"
      },
      "env": {
        "APP_VARIANT": "development",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
      }
    },
    "dev-client-simulator": {
      "extends": "dev-client",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "distribution": "store",
      "releaseChannel": "prod-v2",
      "cache": {
        "key": "3"
      },
      "env": {
        "APP_VARIANT": "production",
        "CHECK_VERSION": "true",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
      }
    },
    "simulator": {
      "extends": "production",
      "releaseChannel": "prod-v2",
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "dev-client": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44"
      }
    },
    "development": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44"
      }
    },
    "preview": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44"
      }
    },
    "production": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44",
        "ascAppId": "1602033154"
      }
    }
  }
}
